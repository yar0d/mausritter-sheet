{
  "name": "mausritter-sheet",
  "version": "1.10.1",
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "serve": "vue-cli-service serve --port 8801",
    "serve:docker": "docker run -it -p 8765:8765 mausritter-sheet",
    "serve:tauri": "tauri dev",
    "build": "vue-cli-service build",
    "build:docker": "docker build --tag mausritter-sheet .",
    "build:tauri": "vue-cli-service build && yarn tauri build",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "release": "echo \"Future version will be:\" ; yarn release:local -- --dry-run | grep \"tagging release\" | cut -d\" \" -f4 | cut -c2- ",
    "release:local": "standard-version",
    "release:publish": "yarn release:local && git push --follow-tags",
    "tauri": "tauri"
  },
  "dependencies": {
    "@mdi/font": "^5.9.55",
    "@tauri-apps/api": "^1.0.0-beta.5",
    "core-js": "^3.16.1",
    "hash.js": "^1.1.7",
    "uuencode": "^0.0.4",
    "uuid": "^8.3.2",
    "vue": "^3.2.2",
    "vue-i18n": "^9.1.7",
    "vuedraggable": "^4.0.3",
    "vuex": "^4.0.2",
    "wave-ui": "^2.23.0"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^2.1.2",
    "@tauri-apps/cli": "^1.0.0-beta.6",
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/compiler-sfc": "^3.2.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.16.0",
    "standard-version": "^9.3.1",
    "vue-cli-plugin-i18n": "^2.1.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "glob-parent": "^5.1.2",
    "nth-check": "^2.0.1",
    "semver-regex": "^3.1.3",
    "set-value": "^4.0.1",
    "trim-newlines": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
